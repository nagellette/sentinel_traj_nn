  "augment_deg": 45,
  "overlap": 0.2,
  "shuffle": false,
  "seed": 10,
  "epoch_limit": 20064,
  "test_model": false,
  "test_model_count": 20,
  "validation_model_count": 2,
  "srcnn_count": 0,
  "fusion_type": "concat"
}
